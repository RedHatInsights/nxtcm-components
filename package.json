{
  "name": "nxtcm-components",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "prettier:fix": "prettier --write '{src,cypress,scripts}/**/*.{js,ts,mjs,jsx,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ct": "playwright test -c playwright-ct.config.ts",
    "test:ct:watch": "playwright test -c playwright-ct.config.ts --ui",
    "test:ct:debug": "playwright test -c playwright-ct.config.ts --debug",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@patternfly/patternfly": "^6.4.0",
    "@patternfly/react-component-groups": "^6.3.0",
    "@patternfly/react-core": "^6.0.0",
    "@patternfly/react-icons": "^6.3.1",
    "@patternfly/react-styles": "^6.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@playwright/experimental-ct-react": "^1.56.1",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/react-vite": "^9.1.10",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.9.1",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.3.0",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "storybook": "^9.1.10",
    "typescript": "^5.2.2",
    "vite": "^5.4.0"
  }
